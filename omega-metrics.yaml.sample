---
numCPU: 1
host: "0.0.0.0"
port: 9005
debugging: false
omega_app_host: "http://app"
omega_app_port: 6080
healthCheck: 60
log:
    console: true
    file: "/var/log/omega/omega-metrics.log"
    level: "debug"
    formatter: "%Date(2006-01-02 15:04:05Z07:00) [%LEVEL] %Msg%n"
    fileSize: 5000000

    poolsize: 100
mq:
    user: "guest"
    password: "guest"
    host: "rmq"
    port: 5672
db:
    user: "root"
    password: "root"
    host: "influxdb"
    port: 5008
    database: "shurenyun"
    query_default_duration: "15m"  # 's' seconds, 'm' minutes, 'h' hours, 'd' days, 'w' weeks
